{
  "name": "rxjs-schedulers",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "npm run main:serve",
    "test": "npm run main:test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "main:test": "ng test",
    "main:serve": "ng serve --project rxjs-schedulers",
    "main:serve:custom": "http-server dist/rxjs-schedulers",
    "main:build": "ng build --project=rxjs-schedulers",
    "lib:build": "ng build --project=angular-signature-pad",
    "element:build": "ng build --project=signature-pad-element --prod --output-hashing=none",
    "element:merge": "cat dist/signature-pad-element/{runtime,main}.js > dist/signature-pad-element/angular-element.js",
    "element:merge:standalone": "cat dist/signature-pad-element/{runtime,polyfills,main}.js > dist/signature-pad-element/angular-element.js",
    "element:gzip": "cat dist/signature-pad-element/angular-element.js | gzip > dist/signature-pad-element/angular-element.js.gz",
    "element:copy": "cat dist/signature-pad-element/angular-element.js > src/assets/elements/angular-element.js",
    "element:update": "npm run element:build && npm run element:merge && npm run element:copy",
    "element:serve": "http-server dist/signature-pad-element/ --gzip"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "6.0.0",
    "@angular/cdk": "^6.0.0",
    "@angular/cli": "^6.0.0",
    "@angular/common": "6.0.0",
    "@angular/compiler": "6.0.0",
    "@angular/core": "6.0.0",
    "@angular/elements": "^6.0.0",
    "@angular/forms": "6.0.0",
    "@angular/http": "6.0.0",
    "@angular/material": "^6.0.0",
    "@angular/platform-browser": "6.0.0",
    "@angular/platform-browser-dynamic": "6.0.0",
    "@angular/pwa": "^0.6.0",
    "@angular/router": "6.0.0",
    "@angular/service-worker": "6.0.0",
    "@webcomponents/custom-elements": "^1.1.0",
    "core-js": "^2.5.4",
    "document-register-element": "^1.7.2",
    "rxjs": "6.1.0",
    "signature_pad": "^2.3.2",
    "zone.js": "^0.8.24"
  },
  "devDependencies": {
    "@angular/compiler-cli": "6.0.0",
    "@angular-devkit/build-angular": "~0.5.0",
    "typescript": "~2.7.2",
    "@angular-devkit/build-ng-packagr": "~0.6.0",
    "@angular/language-service": "6.0.0",
    "@types/jasmine": "~2.8.6",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.2.1",
    "http-server": "^0.11.1",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~1.7.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~1.4.2",
    "karma-jasmine": "~1.1.1",
    "karma-jasmine-html-reporter": "^0.2.2",
    "ng-packagr": "^3.0.0-rc.2",
    "protractor": "~5.3.0",
    "ts-node": "~5.0.1",
    "tsickle": ">=0.25.5",
    "tslib": "^1.7.1",
    "tslint": "~5.9.1"
  }
}
