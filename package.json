{
  "name": "rxjs-schedulers",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "npm run main:serve",
    "test": "npm run main:test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "main:test": "ng test",
    "main:serve": "ng serve --project rxjs-schedulers",
    "main:serve:custom": "http-server dist/rxjs-schedulers",
    "main:build": "ng build --project=rxjs-schedulers",
    "lib:build": "ng build --project=angular-signature-pad",
    "element:build": "ng build --project=elements --prod --output-hashing=none",
    "element:merge": "cat dist/elements/{runtime,polyfills,scripts,main}.js > dist/elements/elements.js",
    "element:copy": "cat dist/elements/elements.js > src/assets/elements/elements.js",
    "element:gzip": "cat dist/elements/elements.js | gzip > dist/elements/elements.js.gz",
    "element:update": "npm run element:build && npm run element:merge && npm run element:copy",
    "element:build:standalone": "ng build --project=elements-standalone --prod --output-hashing=none",
    "element:merge:standalone": "cat dist/elements/{runtime,polyfills,scripts,main}.js > dist/elements/elements-standalone.js",
    "element:gzip:standalone": "cat dist/elements/elements-standalone.js | gzip > dist/elements/elements-standalone.js.gz",
    "element:update:standalone": "npm run element:build:standalone && npm run element:merge:standalone",
    "element:serve": "http-server dist/elements/ --gzip",
    "docs:update": "cat dist/elements/elements-standalone.js > docs/elements-standalone.js && cat dist/elements/elements-standalone.js > docs/elements-standalone.js && cat dist/elements/styles.css > docs/styles.css"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "7.0.2",
    "@angular/cdk": "^7.0.2",
    "@angular/cli": "7.0.4",
    "@angular/common": "7.0.2",
    "@angular/compiler": "7.0.2",
    "@angular/core": "^7.0.2",
    "@angular/elements": "^7.0.2",
    "@angular/forms": "7.0.2",
    "@angular/http": "7.0.2",
    "@angular/material": "^7.0.2",
    "@angular/platform-browser": "7.0.2",
    "@angular/platform-browser-dynamic": "7.0.2",
    "@angular/pwa": "^0.10.4",
    "@angular/router": "7.0.2",
    "@angular/service-worker": "7.0.2",
    "@webcomponents/custom-elements": "^1.2.1",
    "@webcomponents/webcomponentsjs": "^2.1.3",
    "core-js": "^2.5.7",
    "document-register-element": "^1.13.1",
    "rxjs": "6.3.3",
    "signature_pad": "2.3.2",
    "zone.js": "^0.8.24"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.10.4",
    "@angular-devkit/build-ng-packagr": "~0.10.4",
    "@angular/compiler-cli": "7.0.2",
    "@angular/language-service": "7.0.2",
    "@types/jasmine": "~2.8.9",
    "@types/jasminewd2": "~2.0.5",
    "@types/node": "~10.12.2",
    "codelyzer": "~4.5.0",
    "http-server": "^0.11.1",
    "jasmine-core": "~3.3.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.1.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.4",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^1.4.0",
    "ng-packagr": "^4.4.0",
    "protractor": "~5.4.1",
    "ts-node": "~7.0.1",
    "tsickle": "0.33.1",
    "tslib": "^1.9.3",
    "tslint": "~5.11.0",
    "typescript": "^3.1.6"
  }
}
